{
  "name": "renameit",
  "displayName": "ReNameIt",
  "description": "An AI based file renamer",
  "publisher": "DeepakJoshi",
  "repository": "https://github.com/Deepak-png981/Renamer-vscode",
  "icon": "images/ReNameIt.jpg",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "renamer.improveFileName",
        "title": "ReNameIt: Improve the name of the current file"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ReNameIt Settings",
      "properties": {
        "renameit.namingConvention": {
          "type": "string",
          "default": "camelCase",
          "enum": [
            "camelCase",
            "PascalCase",
            "snake_case",
            "kebab-case",
            "UPPER_SNAKE_CASE",
            "Train-Case",
            "dot.notation",
            "HungarianNotation",
            "lisp-case"
          ],
          "description": "Choose the naming convention for renamed files."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/circular-json": "^0.4.0",
    "axios": "^1.7.7",
    "circular-json": "^0.5.9",
    "prompt-sync": "^4.2.0"
  }
}
